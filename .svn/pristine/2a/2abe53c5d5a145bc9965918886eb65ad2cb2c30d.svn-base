package com.jtmproject.task;

/**
 * This class stores a shortcut for desktop
 * @author Javier Tejedor
 */
public class TagAddShortcutDesktop {

	private String name;
	private String file;
	
	/**
	 * constructor
	 * @param name
	 * @param file
	 */
	public TagAddShortcutDesktop(String name, String file) {
		this.name = name;
		this.file = file;
	}
	
	/**
	 * this returns the name of the shortcut
	 * @return
	 */
	public String getName() {
		return name;
	}
	
	/**
	 * this returns the path of the file
	 * @return
	 */
	public String getFile() {
		return file;
	}
}
